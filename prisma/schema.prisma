generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model specialists {
  id         Int      @id @default(autoincrement())
  name       String
  rating     Float
  reviews    Int
  location   String
  experience String
  image      String
  bio        String
  badges     String[] @db.Text
}